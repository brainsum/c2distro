{
    "name": "drupal/c2distro",
    "description": "Distribution for quickly starting a new IFRC project system with Drupal",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mester Peter",
            "email": "pmester@brainsum.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/dropzonejs": "^2.0.0",
        "drupal/entity_browser": "^2.3.0",
        "drupal/entity_embed": "^1.1.0",
        "drupal/exif": "^2.2.0",
        "drupal/field_group": "^3.1.0",
        "drupal/filehash": "1.4.0",
        "drupal/focal_point": "^1.5.0",
        "drupal/image_effects": "^3.0.0",
        "drupal/inline_entity_form": "^1.0.0",
        "drupal/jsonapi_extras": "^3.16.0",
        "drupal/maxlength": "^1.0.0",
        "drupal/media_entity_imagemagick": "^3.0.0",
        "drupal/php_ffmpeg": "^1.1.0",
        "drupal/redirect_after_login": "^2.7.0",
        "drupal/search_api": "^1.19.0",
        "drupal/select2": "^1.12.0",
        "drupal/taxonomy_access_fix": "^3.0.0",
        "drupal/term_merge": "^1.0.0",
        "drupal/userprotect": "^1.1.0",
        "drupal/video_embed_field": "^2.4.0",
        "npm-asset/dropzone": "^5.7.6",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "npm-asset/select2": "^4.0.13"
    }
}
