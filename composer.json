{
    "name": "brainsum/c2distro",
    "description": "Distribution for quickly starting a new c2distro project system with Drupal",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mester Peter",
            "email": "pmester@brainsum.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "drupal/block_field": "^1.0@RC",
        "drupal/calendar": "^1.0@alpha",
        "drupal/classy_paragraphs": "^1.0@beta",
        "drupal/ctools": "^3.6",
        "drupal/default_content": "^2.0@alpha",
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/field_group": "^3.1",
        "drupal/metatag": "^1.16",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/search_api": "^1.19.0",
        "drupal/structure_sync": "^2.0",
        "drupal/svg_image": "^1.14",
        "drupal/token": "^1.9",
        "drupal/transliterate_filenames": "^1.5",
        "drupal/warmer": "^2.0",
        "drupal/warmer_entity": "^2.0"
    }
}
